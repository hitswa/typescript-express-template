{
  "name": "typescriptexpress",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "start": "cross-env NODE_ENV=production npm run build && node dist/index.js",
    "start:production": "cross-env NODE_ENV=production npm run build && node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "dev1": "cross-env NODE_ENV=development ts-node src/index.ts",
    "start:local": "cross-env NODE_ENV=local ts-node src/index.ts",
    "test:compilabile": "npx tsc --noEmit",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test:jest": "jest",
    "test:e2e:cypress": "npx cypress run",
    "test:e2e:playwright": "npx playwright test",
    "test:e2e:selenium": "npx ts-node src/Tests/E2E/Example3.test.ts"
  },
